        "type": "git",
        "url": "git+https://github.com/lanbin/parcel-vue.git"
    },
    "keywords": [
        "vuejs",
        "parcel"
    ],
    "author": "lanbin1987@gmail.com",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/lanbin/parcel-vue/issues"
    },
    "homepage": "https://github.com/lanbin/parcel-vue#readme",
    "dependencies": {
        "vue": "^2.5.9"
    },
    "devDependencies": {
        "cross-env": "^5.1.1"
    }
}
