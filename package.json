{
    "name": "parcel-vue",
    "version": "1.0.0",
    "description": "A Vuejs template project packed by parcel",
    "main": "index.js",
    "scripts": {
        "start": "parcel -p 2345 index.js",
        "build": "cross-env NODE_ENV=production parcel build index.js --no-minify -d build/output"
    },
